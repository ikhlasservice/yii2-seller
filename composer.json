{
    "name": "andasoft/yii2-core",
    "description": "Yii 2 for base system",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/andasoft/yii2-core.git",
        "issues": "https://github.com/andasoft/yii2-core/issues"
    },
    "authors": [
        {
            "name": "Anda Soft",
            "email": "kuakling@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*"
    },
    "autoload": {
        "psr-4": {
            "anda\\core\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
